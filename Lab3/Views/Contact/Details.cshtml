@{
}

<h1>Szczegóły</h1>
<br />
<div style="padding: 20px; background-color: #578CB5;
               border-radius: 25px; box-shadow: 0 0 15px;
               "

<h5>Imię: @Model.Name</h5>
<h5>Email: @Model.Email</h5>
<h5>Numer telefonu: @Model.Phone</h5>
